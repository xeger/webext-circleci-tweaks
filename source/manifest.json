{
	"name": "Quarantini for CircleCI",
	"version": "0.0.0",
	"description": "One-button followup to broken builds and failing tests",
	"homepage_url": "https://github.com/xeger/quarantini",
	"manifest_version": 2,
	"minimum_chrome_version": "74",
	"applications": {
		"gecko": {
			"id": "quarantini@xeger.github.io",
			"strict_min_version": "67.0"
		}
	},
	"icons": {
		"128": "icon.png"
	},
	"permissions": [
		"storage"
	],
	"options_ui": {
		"chrome_style": true,
		"page": "options.html"
	},
	"background": {
		"persistent": false,
		"scripts": [
			"background.js"
		]
	},
	"content_scripts": [
		{
			"matches": [
				"https://app.circleci.com/*"
			],
			"js": [
				"content-circleci-tests.js"
			]
		}
	]
}
